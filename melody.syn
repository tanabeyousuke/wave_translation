module_num: !特定ユニットの数
  	    lfo 1; !lfoの個数
  	    efc 0; !エフェクトの個数

setting:
  osc sin 100;

  env atk 200;
  env dec 8820;
  env sus 0.5;
  env rel 22050;

  lfo sin r1 r9 r10; !波形、周波数、振幅、オフセット、出力 

  efc low 3000 15 !エフェクト 入力数は種類によって異なる
  efc hig 5000 20
  efc bit 15
  
  amp r5; !最終的にどれぐらいの大きさで音を出すか決めるアンプ

play:
  mov r5 30;

  key g 4 22040;
  rst 22040;

  key e 4 22050;
  rst 22050;
  
  key c 4 22050;
  rst 22050;

  key g 3 22050;
  rst 22050;

end: