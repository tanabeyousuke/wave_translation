module_num: !特定ユニットの数
  	    lfo 1; !lfoの個数
  	    efc 3; !エフェクトの個数
	    vce 3; !同時発音数

setting:
  osc sin 30; !オシレータの音量
  osc del 30;

  env atk r5;
  env dec r6;
  env sus r7;
  env rel r8;
  env out 399;

  lfo sin r1 r9 r10; !波形、周波数、振幅、オフセット、出力 

  efc low 3000 15; !エフェクト 入力数は種類によって異なる
  efc hig 5000 20;
  efc bit 15;
  
  amp r5; !最終的にどれぐらいの大きさで音を出すか決めるアンプ

play:
  mov r10 1.0; !レジスタへの代入
  
  kon 1 dis;