module_num: !特定ユニットの数
  	    lfo 1; !lfoの個数
  	    efc 0; !エフェクトの個数

setting:
  osc sin 100;

  env atk 200;
  env dec 8820;
  env sus 1;
  env rel 22050;

  lfo sqr 1.5 50 50 r5; !波形、周波数、振幅、オフセット、出力 

  efc low 3000 15 !エフェクト 入力数は種類によって異なる
  efc hig 5000 20

  amp r5; !最終的にどれぐらいの大きさで音を出すか決めるアンプ

play:

  key g 4 99999;

end: